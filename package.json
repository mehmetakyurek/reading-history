{
  "name": "reading-history",
  "version": "0.1.0",
  "homepage": ".",
  "main": "electron/build/index.js",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "debounce": "^1.2.1",
    "faker": "^5.4.0",
